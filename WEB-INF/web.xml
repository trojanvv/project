<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>DBview_servlet</servlet-name>
        <servlet-class>main.DBview_servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrationServlet</servlet-name>
        <servlet-class>main.registrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usersServlet</servlet-name>
        <servlet-class>main.usersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editUserServlet</servlet-name>
        <servlet-class>main.editUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addPcServlet</servlet-name>
        <servlet-class>main.addPcServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pcServlet</servlet-name>
        <servlet-class>main.pcServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>DBview_servlet</servlet-name>
        <url-pattern>/view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrationServlet</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usersServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editUserServlet</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addPcServlet</servlet-name>
        <url-pattern>/addpc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pcServlet</servlet-name>
        <url-pattern>/pc</url-pattern>
    </servlet-mapping>


</web-app>